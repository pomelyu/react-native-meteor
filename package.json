{
  "name": "@pomelyu/react-meteor-client",
  "version": "1.2.4",
  "description": "Full Meteor Client for React",
  "main": "src/Meteor.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel index.js -d dist && babel src -d dist/src && babel lib -d dist/lib && cp package.json LICENSE README.md dist",
    "prepublish": "npm run build && mv index.js index.backup.js && cp index.package.js index.js && mv src src.backup",
    "postpublish": "cp index.backup.js index.js && mv src.backup src && rimraf index.backup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pomelyu/react-native-meteor.git"
  },
  "keywords": [
    "react-component",
    "ddp",
    "meteor",
    "react"
  ],
  "license": "MIT",
  "dependencies": {
    "base-64": "^0.1.0",
    "crypto-js": "^3.1.6",
    "ejson": "^2.1.2",
    "minimongo-cache": "0.0.48",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "trackr": "^2.0.2",
    "underscore": "^1.8.3",
    "wolfy87-eventemitter": "^4.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.7.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "mock-socket": "^2.0.0",
    "rimraf": "^2.6.2",
    "sinon": "^1.17.3"
  },
  "bugs": {
    "url": "https://github.com/pomelyu/react-native-meteor/issues"
  },
  "homepage": "https://github.com/pomelyu/react-native-meteor#readme",
  "directories": {
    "example": "example",
    "lib": "lib",
    "test": "test"
  },
  "author": ""
}
